Level1 = function(game) {

    this.game = game;
    this.map = null;
    this.layer = null;

}

Level1.prototype = {

    preload: function() {
        this.game.load.tilemap('test', './assets/test.json', null, Phaser.Tilemap.TILED_JSON);
        this.game.load.image('tiles', './assets/tilesetbasics.png');
    },

    create: function() {

        // Skapar tilemappen
        this.map = this.game.add.tilemap('test');
        this.map.addTilesetImage('tilesetbasics', 'tiles');

        // Kollision med allt
        this.map.setCollisionByExclusion([]);

        this.layer = this.map.createLayer('Ground');
        this.layer.resizeWorld();
        this.layer.debug = true;

    },

    update: function() {

    }

}